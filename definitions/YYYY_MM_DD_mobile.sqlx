config {
    type: "operations",
    schema: "lighthouse",
    name: "2024_06_01_mobile"
}

SELECT page AS url, lighthouse AS report
FROM `all.pages`
WHERE date = '2024-06-01' AND
client = 'mobile' AND
is_root_page AND
lighthouse IS NOT NULL AND
LENGTH(lighthouse) <= 2 * 1024 * 1024 -- legacy tables have a different limit